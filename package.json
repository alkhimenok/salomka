{
	"name": "vid-eco-pro",
	"version": "1.0.0",
	"description": "Website for the sale of Eco paper straws for drinks that do not harm nature.",
	"main": "app.js",
	"scripts": {
		"webpackdev": "cross-env NODE_ENV=development webpack",
		"webpackbuild": "cross-env NODE_ENV=production webpack",
		"webpackwatch": "cross-env NODE_ENV=development webpack-dev-server --open",
		"serverstart": "node app.js",
		"serverwatch": "nodemon app.js",
		"dev": "concurrently \"npm run webpackdev\" \"npm run serverwatch\"",
		"prod": "concurrently \"npm run webpackbuild\" \"npm run serverstart\"",
		"stats": "webpack --json > stats.json && webpack-bundle-analyzer stats.json"
	},
	"browserslist": "> 0.25%, not dead",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kirill-leonidovich/vid-eco-pro.git"
	},
	"keywords": [
		"website",
		"ecological",
		"paper-straws"
	],
	"author": "Kirill Alkhimenok <alkhimenok.kirill.leonidovich@gmail.com>",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/kirill-leonidovich/vid-eco-pro/issues"
	},
	"homepage": "https://github.com/kirill-leonidovich/vid-eco-pro#readme",
	"devDependencies": {
		"@babel/core": "^7.16.0",
		"@babel/polyfill": "^7.12.1",
		"@babel/preset-env": "^7.16.0",
		"autoprefixer": "^10.4.0",
		"babel-loader": "^8.2.3",
		"clean-webpack-plugin": "^4.0.0",
		"concurrently": "^6.3.0",
		"cross-env": "^7.0.3",
		"css-loader": "^6.5.1",
		"html-loader": "^3.0.1",
		"html-webpack-plugin": "^5.5.0",
		"mini-css-extract-plugin": "^2.4.4",
		"node-sass": "^6.0.1",
		"nodemon": "^2.0.14",
		"optimize-css-assets-webpack-plugin": "^6.0.1",
		"postcss": "^8.3.11",
		"postcss-loader": "^6.2.0",
		"sass-loader": "^12.3.0",
		"style-loader": "^3.3.1",
		"terser-webpack-plugin": "^5.2.5",
		"webpack": "^5.62.1",
